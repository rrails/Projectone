<div class = 'detail'>
  <% @attendees.each do |attendee| %>
    <ul class='item'>
      <li><%= attendee.user_id %></li>
      <li><%= attendee.call_id %></li>
      <li><%= attendee.meetingdate %></li>
      <hr>
    </ul>
    <!-- this goes to a form to show the edit fields -->
<%=  link_to 'Edit', edit_attendee_path(attendee)%>
<!-- This deletes the record without showing the data -->
<%= link_to 'delete', attendee, :method => :delete %>
  <% end %>
</div>

